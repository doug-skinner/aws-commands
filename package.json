{
    "name": "aws-commands",
    "version": "0.4.2",
    "description": "An unoficial node.js command line wrapper around my commonly used AWS commands",
    "main": "src/index.js",
    "scripts": {
        "build": "npm-run-all format check lint",
        "check": "prettier src/ --check",
        "format": "npm-run-all format:*",
        "format:lint": "eslint src/ --fix",
        "format:style": "prettier src/ --write",
        "lint": "eslint src/",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "bin": {
        "awsCommands": "src/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/doug-skinner/aws-commands.git"
    },
    "keywords": [],
    "author": "Doug Skinner",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/doug-skinner/aws-commands/issues"
    },
    "homepage": "https://github.com/doug-skinner/aws-commands#readme",
    "devDependencies": {
        "eslint": "^7.2.0",
        "eslint-config-prettier": "^6.15.0",
        "npm-run-all": "^4.1.5",
        "prettier": "2.2.0"
    },
    "dependencies": {
        "aws-sdk": "^2.700.0",
        "aws4": "^1.10.0",
        "axios": "^0.21.1",
        "commander": "^5.1.0",
        "winston": "^3.2.1"
    }
}
